a = [1, 2, 3, 4, 5]
b = [10, 20, 30, 40, 50]
k = 2

new_list = []

# Step 1: Insert last k elements at front (reversed order)
for i in range(k):
    new_list.insert(0, a[len(a) - 1 - i])

print(new_list)

# Step 2: Add remaining front part of a
new_list.extend(a[0:len(a) - k])
print(new_list)

# Step 3: Add b element-wise
for i, item in enumerate(b):
    new_list[i] += item

print(new_list)
